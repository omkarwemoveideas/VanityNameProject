{
  "name": "VanityNameRegistry",
  "version": "1.0.0",
  "description": "A vanity name registering system resistant against frontrunning.",
  "license": "MIT",
  "scripts": {
    "generate": "truffle compile && typechain --target truffle-v5 './build/**/*.json'",
