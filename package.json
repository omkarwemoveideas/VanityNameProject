    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0"
  },
  "devDependencies": {
    "@typechain/truffle-v5": "^7.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "truffle": "^5.5.4",
    "truffle-assertions": "^0.9.2",
    "ts-node": "^10.7.0",
    "typechain": "^7.0.1",
    "typescript": "^4.6.2"
  }
}
